{
  "name": "@comandeer/babel-plugin-banner",
  "version": "0.0.0",
  "description": "Simple Babel plugin to prepend something to the beginning of transformed code.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "lint": "eslint src/**/*.js tests/*.js",
    "pretest": "npm run lint",
    "test": "mocha tests/index.js",
    "preversion": "npm test",
    "postversion": "git push origin && git push origin --tags"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Comandeer/babel-plugin-banner.git"
  },
  "keywords": [
    "babel",
    "plugin",
    "banner"
  ],
  "author": "Comandeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Comandeer/babel-plugin-banner/issues"
  },
  "homepage": "https://github.com/Comandeer/babel-plugin-banner#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "mocha": "^3.2.0"
  }
}
